<section class="card">
  <h2>History</h2>
  <p class="meta">Unified event history for this profile. Includes media previews/actions for captured posts and downloaded stories.</p>
  <div
    class="tabulator-shell"
    data-controller="profile-events-table"
    data-profile-events-table-url-value="<%= events_instagram_profile_path(profile, format: :json) %>"
    data-profile-events-table-profile-id-value="<%= profile.id %>"
    data-profile-events-table-profile-username-value="<%= profile.username %>"
  >
    <div class="tabulator-host" data-profile-events-table-target="table"></div>
  </div>
</section>
